{
  "name": "get-firefox",
  "version": "3.0.1",
  "description": "Download the latest Firefox from the command line",
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint index.js lib/ bin/get-firefox test/ --ext js,json",
    "lint:ec": "eclint check $(git ls-files)",
    "lint": "npm run lint:js && npm run lint:ec",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "start": "node bin/get-firefox"
  },
  "keywords": [
    "fennec",
    "fxa",
    "firefox",
    "android",
    "nightly",
    "download",
    "mozilla-central",
    "trunk",
    "devedition",
    "esr"
  ],
  "author": "freaktechnik (http://humanoids.be)",
  "license": "MPL-2.0",
  "dependencies": {
    "chalk": "^3.0.0",
    "decompress": "^4.2.0",
    "into-stream": "^5.1.1",
    "is-ci": "^2.0.0",
    "listr": "^0.14.3",
    "meow": "^6.1.0",
    "moz-download-url": "^2.0.0",
    "node-fetch": "^2.6.0",
    "sha": "^3.0.0",
    "stream-to-promise": "^2.2.0",
    "taskcluster-client": "^28.0.0"
  },
  "bin": {
    "get-firefox": "./bin/get-firefox"
  },
  "bugs": {
    "url": "https://github.com/freaktechnik/get-firefox/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freaktechnik/get-firefox.git"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@freaktechnik/eslint-config-node": "^7.1.0",
    "@freaktechnik/eslint-config-test": "^7.1.0",
    "ajv": "^6.12.0",
    "ava": "^3.5.1",
    "codecov": "^3.6.1",
    "eclint": "^2.8.1",
    "eslint": "^6.8.0",
    "nyc": "^15.0.0",
    "tmp-promise": "^2.0.2"
  },
  "ava": {
    "babel": true,
    "timeout": "1m"
  }
}

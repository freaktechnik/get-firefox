    "node": ">=12.22.0"
  },
  "devDependencies": {
    "@freaktechnik/eslint-config-node": "^9.0.3",
    "@freaktechnik/eslint-config-test": "^9.0.1",
    "ajv": "^8.12.0",
    "ava": "^5.1.1",
    "eclint": "^2.8.1",
    "eslint": "^8.32.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "nyc": "^15.1.0",
    "tmp-promise": "^3.0.3"
  },
  "ava": {
    "timeout": "1m"
  }

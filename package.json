  "license": "MPL-2.0",
  "dependencies": {
    "chalk": "^5.2.0",
    "decompress": "^4.2.1",
    "into-stream": "^7.0.0",
    "is-ci": "^3.0.1",
    "listr": "^0.14.3",
    "meow": "^11.0.0",
    "moz-download-url": "^2.1.0",
    "node-fetch": "^3.3.0",
    "sha": "^3.0.0",
    "stream-to-promise": "^3.0.0",
    "taskcluster-client": "^47.0.2"
  },
  "bin": {
